<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    <h2>Usuarios</h2>
        <ul>
        <% usuarios.forEach(u => { %>
            <li><%= u.nombre %> - <%= u.dni %></li>
        <% }) %>
        </ul>

        <h2>Asignaturas</h2>
        <ul>
        <% asignaturas.forEach(a => { %>
            <li><%= a.nombre %> - <%= a.codigo %></li>
        <% }) %>
        </ul>
        <!-- igual con semestres, portafolios, etc -->
        <form method="POST" action="/admin/portafolios/guardar">
        <input type="hidden" name="datosProcesados" value='<%= JSON.stringify({usuarios, asignaturas, ...}) %>'>
        <button type="submit">Confirmar y Guardar</button>
        </form>
 
</body>
</html>